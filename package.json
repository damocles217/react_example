		"jest": "^28.1.1",
		"jest-environment-jsdom": "^28.1.1",
		"jsdom": "^19.0.0",
		"prettier": "^2.6.2",
		"sass": "^1.52.3",
		"ts-jest": "^28.0.5",
		"ts-node": "^10.8.1",
		"typescript": "^4.6.3",
		"vite": "^2.9.9"
	}
